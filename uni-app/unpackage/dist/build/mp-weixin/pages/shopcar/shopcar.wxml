<view class="data-v-118dc800"><block wx:if="{{cartlist.length>0}}"><view class="data-v-118dc800"><checkbox-group data-event-opts="{{[['change',[['handleChangeChecked',['$event']]]]]}}" bindchange="__e" class="data-v-118dc800"><label class="data-v-118dc800"><checkbox value="随意的值" checked="{{$root.m0}}" class="data-v-118dc800"></checkbox><text class="data-v-118dc800">全选</text></label></checkbox-group><block wx:for="{{cartlist}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['longpress',[['handleShowModal',['$0'],[[['cartlist','id',item.id,'id']]]]]]]}}" class="item data-v-118dc800" bindlongpress="__e"><label class="data-v-118dc800"><checkbox checked="{{item.checked}}" data-event-opts="{{[['tap',[['handleChecked',['$0'],[[['cartlist','id',item.id,'id']]]]]]]}}" bindtap="__e" class="data-v-118dc800"></checkbox></label><image src="{{'http://localhost:3000'+item.good.poster}}" mode="widthFix" class="data-v-118dc800"></image><view class="data-v-118dc800"><view class="goodname data-v-118dc800">{{''+item.good.name+''}}</view><view class="data-v-118dc800">{{''+item.good.price+''}}</view></view><view class="calculate data-v-118dc800"><text data-event-opts="{{[['tap',[['handleMinus',['$0'],[[['cartlist','id',item.id,'id']]]]]]]}}" bindtap="__e" class="data-v-118dc800">-</text><input type="text" disabled value="{{item.number}}" class="data-v-118dc800"/><text data-event-opts="{{[['tap',[['handleAdd',['$0'],[[['cartlist','id',item.id,'id']]]]]]]}}" bindtap="__e" class="data-v-118dc800">+</text></view></view></block><view class="data-v-118dc800">{{'总金额： '+computedSum+''}}</view></view></block><block wx:else><view class="data-v-118dc800">购物网空空如也</view></block></view>